<?xml version="1.0" encoding="utf-8"?>
<customization_form>
  <category name="application">
    <module name="FOTA">
      <function name="settings">
        <set name="single">
          <item name="Enable_FOTA_UI" type="boolean">yes</item>
          <item name="Enable_Manual_Check" type="boolean">yes</item>
          <item name="Default_Download_Wifi" type="boolean">no</item>
        </set>
      </function>
    </module>
    <module name="roaming">
      <function name="default_roaming">
        <set name="plenty" max="5">
          <item name="data_roaming">0</item>
        </set>
      </function>
    </module>
    <module name="Crash_Report">
      <function name="setting">
        <set name="single">
          <item name="turn_off_report" type="boolean">no</item>
          <item name="turn_off_application_log" type="boolean">no</item>
        </set>
      </function>
    </module>
  </category>
  <category name="Settings">
    <module name="list">
      <function name="regulatory_generic_list">
        <set name="single">
          <!-- %%UAND000157_SAND000157_File Pool%% -->
          <item name="country_1">Americas</item>
          <!-- %%UAND000157_SAND000157_File Pool%% -->
          <item name="icon_2">/system/customize/resource/rp_fcc.png</item>
          <!-- %%UAND000157_SAND000157_File Pool%% -->
          <item name="text_3">FCC ID: NM82PS6500</item>
          <!-- %%UAND000157_SAND000157_File Pool%% -->
          <item name="text_4">IC: 4115A-2PS6500</item>
          <!-- %%UAND000157_SAND000157_File Pool%% -->
          <item name="icon_5">/system/customize/resource/rp_weee.png</item>
        </set>
      </function>
    </module>
  </category>
  <category name="SettingsProvider">
    <module name="res">
      <function name="values">
        <set name="single">
          <item name="def_2g_on">3</item>
        </set>
      </function>
    </module>
  </category>
  <category name="system">
    <module name="deviceData">
      <function>
        <set name="single">
          <item name="device_name">HTC 10</item>
        </set>
      </function>
    </module>
    <module name="deviceData1">
      <function>
        <set name="single">
          <item name="sw_number">NA</item>
        </set>
      </function>
    </module>
  </category>
